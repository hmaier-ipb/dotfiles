"----------- 
"   VIMRC   |
"----------- 

"copy to clipboard
noremap <C-y> "+y  

"paste from clipboard
noremap <C-p> "+p  

syntax on
set relativenumber 	" hybrid
set number 		" hybrid
set mouse=a
set ignorecase		"case-insensitive searching
set smartcase
" Disable audible/visual bells
set noerrorbells
set novisualbell
set t_vb=
